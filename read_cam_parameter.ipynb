    "import pickle\n",
    "\n",
    "# 打開並讀取保存的校準數據文件\n",
    "with open('calibration.pckl', 'rb') as f:\n",
    "    cameraMatrix, distCoeffs, rvecs, tvecs = pickle.load(f)\n",
    "\n",
    "# 現在可以使用讀取的數據，例如：\n",
    "print(\"相機矩陣：\", cameraMatrix)\n",
    "print(\"畸變係數：\", distCoeffs)\n",
    "print(\"旋轉向量：\", rvecs)\n",
    "print(\"平移向量：\", tvecs)\n"
   ]
